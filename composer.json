{
    "name": "endroid/image-placeholder-bundle",
    "description": "Endroid Image Placeholder Bundle for Symfony",
    "keywords": ["endroid", "image", "placeholder", "unsplash.it", "placehold.it", "symfony", "bundle"],
    "homepage": "https://github.com/endroid/EndroidImagePlaceholderBundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.0",
        "phpunit/phpunit": "~4.0",
        "symfony/var-dumper": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Bundle\\ImagePlaceholderBundle\\": [ "src/", "tests/" ]
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
