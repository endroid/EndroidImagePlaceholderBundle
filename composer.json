{
    "name": "endroid/image-placeholder-bundle",
    "description": "Endroid Image Placeholder Bundle for Symfony",
    "keywords": ["endroid", "image", "placeholder", "unsplash", "placehold", "placekitten", "symfony", "bundle"],
    "homepage": "https://github.com/endroid/EndroidImagePlaceholderBundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Bundle\\ImagePlaceholderBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Bundle\\ImagePlaceholderBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
