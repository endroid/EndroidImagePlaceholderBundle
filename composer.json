{
    "name": "endroid/image-placeholder",
    "description": "Endroid Image Placeholder",
    "keywords": ["endroid", "image", "placeholder", "unsplash", "placehold", "placekitten", "php"],
    "homepage": "https://github.com/endroid/image-placeholder",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": ">=7.1",
        "endroid/installer": "^1.0.3"
    },
    "require-dev": {
        "endroid/test": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\ImagePlaceholder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\ImagePlaceholder\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
